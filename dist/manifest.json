{
  "manifest_version": 3,
  "name": "FLN - Custom speed for Youtube playback",
  "version": "0.2",
  "description": "Add custom playback speed controls to YouTube videos.",
  "permissions": ["scripting", "activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "host_permissions": ["*://www.youtube.com/*"],
  "action": {
    "default_title": "Speed Booster"
  }
}
